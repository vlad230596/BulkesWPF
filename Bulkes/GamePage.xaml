<Page x:Class="Bulkes.GamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Bulkes"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="GamePage"
      Loaded="gamePageLoaded"
      SizeChanged="gamePageSizeChanged">

      <Canvas x:Name="canvas" Background="White" MouseDown="canvasMouseDown" MouseMove="canvasMouseMove"/>
</Page>
